    <link rel="stylesheet" href="~/tem/css/flaticon.css">
    <link rel="stylesheet" href="~/tem/css/style.css">
    
</head>
<body>
<nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark " id="ftco-navbar">
	    <div class="container">
	      <a class="navbar-brand" href="">Realty Mulad</a>
	      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
	        <span class="oi oi-menu"></span> Menu
	      </button>

	      <div class="collapse navbar-collapse" id="ftco-nav">
	        <ul class="navbar-nav ml-auto">
	          <li class="nav-item active"><a asp-area="" asp-controller="Home" asp-action="Index" class="nav-link">Home</a></li>
	          <li class="nav-item"><a asp-area="" asp-controller="Home" asp-action="Privacy" class="nav-link">About</a></li>
	          <li class="nav-item"><a asp-controller="Realtor" asp-action="DashBoard" class="nav-link" >Agent</a></li>
	           <li class="nav-item"><a asp-controller="Buyer" asp-action="Dashboard" class="nav-link">Client</a></li>
	          <li class="nav-item"><a asp-controller="Home" asp-action="AllProperties" class="nav-link">Properties</a></li>
	          <li class="nav-item"><a class="nav-link" asp-controller="User" asp-action="Login" >Login</a></li>
	          <li class="nav-item"><a class="nav-link">Contact</a></li>
	        </ul>
	      </div>
	    </div>
</nav>

 
 <section  class="ftco-section goto-here">
      <div class="container">
	      <div class="row justify-content-center">
		      <div class="col-md-12 heading-section text-center ftco-animate mb-5">
			      <span class="subheading">Search Result</span>
			      
		      </div>
	      </div>
@if (!string.IsNullOrEmpty(ViewBag.Message))
			{
				<h3 class="mb-2">@ViewBag.Message</h3>
			}
@if(Model!=null){
	      <div class="row">
		   @foreach (var property in Model)
		   {
			   <div class="col-md-4">

				   <div class="property-wrap ftco-animate">
					   <a asp-route-Id="@property.Id" asp-action="Property" asp-controller="Home" class="img" style="background-image: url('/PropertyImages/@property.ImagePath[0]');">
						   <div class="rent-sale">
							   <span class="rent">Sale</span>
						   </div>
						   <p class="price"><span class="orig-price">#@property.PropertyPrice.ToString("N"),</span></p>
					   </a>
					   <div class="text">
						   <ul class="property_list">
							   <h3>@property.Features</h3>
							   <li><span class="flaticon-bed"></span>Bedroom:@property.Bedroom</li>
							   <li><span class="flaticon-bathtub"></span>Toilet:@property.Toilet</li>
							   <li><span class="flaticon-floor-plan"></span>Plot area:@property.LandArea</li>
						   </ul>
						   <span>@property.Address</span>
						   <span class="location">@property.LGA, @property.State</span>
						   <a href="#" class="d-flex align-items-center justify-content-center btn-custom">
							   <span class="fa fa-link"></span>
						   </a>
						   <div class="list-team d-flex align-items-center mt-2 pt-2 border-top">
							   <div class="d-flex align-items-center">
								   <div class="img" style="background-image: url('images/work-1.jpg');"></div>
								   <h3 class="ml-2">Registration No:@property.PropertyRegNumber</h3>
							   </div>
							   <span class="text-right">Property:@property.PropertyType</span>
						   </div>
					   </div>
					      
				   </div>
			   </div>
			     
		   }
	   </div>
}	   
	 </div>
    </section>
  


<div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>


